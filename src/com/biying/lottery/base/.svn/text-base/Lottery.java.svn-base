package com.biying.lottery.base;

import java.util.ArrayList;

public abstract class Lottery {
	private String name;
	private ArrayList<LotteryMode> modes = new ArrayList<LotteryMode>();
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public ArrayList<LotteryMode> getModes() {
		return modes;
	}
	public void setModes(ArrayList<LotteryMode> modes) {
		this.modes = modes;
	}
}
