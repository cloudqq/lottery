package com.biying.lottery.base;

import java.util.ArrayList;

public abstract class LotteryMode 
{
	private Lottery lottery;
	private ArrayList<BallGroup> groups = new ArrayList<BallGroup>();
	private String modeName;
	
	public String getModeName() {
		return modeName;
	}

	public void setModeName(String modeName) {
		this.modeName = modeName;
	}

	public Lottery getLottery() {
		return lottery;
	}

	public void setLottery(Lottery lottery) {
		this.lottery = lottery;
	}

	public ArrayList<BallGroup> getGroups() {
		return groups;
	}
	
	public void setGroups(ArrayList<BallGroup> groups) {
		this.groups = groups;
	}
}
